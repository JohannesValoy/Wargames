<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="addUnit" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="400.0" minWidth="600.0" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Old.AddUnitController">
   <center>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <VBox alignment="CENTER" prefHeight="287.0" prefWidth="233.0" spacing="10.0">
            <Button fx:id="cavalryUnitButton" mnemonicParsing="false" onAction="#cavalryUnitButton" prefHeight="40.0" prefWidth="200.0" style="-fx-border-color: none;" text="Cavalry Unit" />
            <Button fx:id="commanderUnitButton" mnemonicParsing="false" onAction="#commanderUnitButton" prefHeight="40.0" prefWidth="200.0" text="Commander Unit" />
            <Button fx:id="infantryUnitButton" mnemonicParsing="false" onAction="#infantryUnitButton" prefHeight="40.0" prefWidth="200.0" text="Infantry Unit" />
            <Button fx:id="rangeUnitButton" mnemonicParsing="false" onAction="#rangeUnitButton" prefHeight="40.0" prefWidth="200.0" text="Range Unit" />
         </VBox>
         <VBox alignment="CENTER" prefHeight="295.0" prefWidth="189.0" spacing="10.0">
            <TextField fx:id="nameField" maxWidth="200.0" prefHeight="40.0" promptText="Name" />
            <TextField fx:id="healthField" prefHeight="40.0" prefWidth="200.0" promptText="Health" />
         </VBox>
      </HBox>
   </center>
   <bottom>
      <HBox alignment="CENTER" prefHeight="51.0" prefWidth="600.0" spacing="20.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="removeUnitButton" minWidth="-Infinity" mnemonicParsing="false" onAction="#removeUnitButton" prefWidth="100.0" style="-fx-background-color: rgb(210,210,210);" text="Remove Unit" />
            <Button fx:id="addUnitButton" lineSpacing="10.0" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onAction="#addUnitButton" prefWidth="100.0" style="-fx-background-color: rgb(256,256,256);" text="Add Unit" />
         </children></HBox>
   </bottom>
</BorderPane>
